<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.day.schadualer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon_1"
        android:label="@string/app_name"
         >
        <activity
            android:name="com.day.scheduler.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.day.scheduler.ContentActivity"></activity>
        <activity android:name="com.day.scheduler.NodeActivity"></activity>
        <activity android:name="com.day.scheduler.LogCatActivity"></activity>
    </application>

</manifest>
